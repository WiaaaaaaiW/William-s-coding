primes = [2]
a = 3
factors = []
numbers = int(input('Plug in a number to factor: '))
counts = []

for num in primes:
    if numbers <= 50 or numbers > 200:
        factors.append("Number must be between 50 and 200, try again.")
        break
            
while a <= 300:
    is_prime = True
    for prime in primes:
        if a % prime == 0:
            is_prime = False
            break
    if is_prime:
        primes.append(a)
    a = a + 1

if numbers in primes:
    print("This is a prime number")
else:
    number = numbers
    for num in primes:
        count = 0
        while numbers % num == 0:
            count = count + 1
            numbers = numbers / num
        if count > 0:
            factors.append(num)
            counts.append(count)
        if numbers == 1:
            break
    if numbers > 1:
        factors.append(number)
        count.append(1)

    formatted_factors = [f"{factor}^{count}" if count > 1 else f"{factor}" for factor, count in zip(factors, counts)]
    print(f"factors = {' * '.join(formatted_factors)}")

